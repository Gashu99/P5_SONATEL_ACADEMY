}
}
#VERIFICATION Nom
verifnom=function(nom){
k=0
nom1=substring(nom,1,1)
decopositionnom=strsplit(nom,split="")
for (i in  1:nchar(nom)){
if (grepl("[[:alpha:]]",decopositionnom[[1]][i])) {
k=k+1
}
}
if ((k>=2) & (grepl("[[:alpha:]]",nom1))){
return(TRUE)
}
else{
return(FALSE)
}
}

#VERIFICATION DATE
verifdate=function(date){
date=trimws(date)
sep=c(".","|",";"," ","-")
for(i in (1:nchar(date))){
if(substring(date,i,i) %in% sep){
date =gsub(substring(date,i,i),'/',date)
}
}
datedecomp=strsplit(date,split = "/")
if(datedecomp[[1]][2]=='fev'){
datedecomp[[1]][2]=gsub('fev','02',datedecomp[[1]][2])
}
if(datedecomp[[1]][2]=='janvier'){
datedecomp[[1]][2]=gsub('janvier','01',datedecomp[[1]][2])
}
if(datedecomp[[1]][2]=='mars'){
datedecomp[[1]][2]=gsub('mars','03',datedecomp[[1]][2])
}
if(datedecomp[[1]][2]=='Decembre'){
datedecomp[[1]][2]=gsub('Decembre','12',datedecomp[[1]][2])
}
datedecomp=sys.Date()
datefinale=as.Date(datedecomp,"%y/%m/%d",lang="fr")
datefinale=format(datefinale,"%d/%m/%y")
datefinale
}
